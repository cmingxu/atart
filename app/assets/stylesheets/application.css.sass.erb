@import 'c3'
@import "bootstrap-sprockets"
@import "bootstrap"

body
  background-color: #ccd1e7
.fc-container
  padding-left: 160px
  padding-top: 50px
.fc-sidebar
  width: 160px
  position: absolute
  left: 0px
  background-color: white
  ul
    width: 100%
    min-height: 1000px
    li
      width: 100%

.head
  height: 50px
  position: fixed
  top: 0px
  border-top: 3px solid orange
  width: 100%
  background-color: white
  box-shadow: 5px 0px 5px rgba(blue, .3)
