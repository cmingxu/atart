body { background: #f5f6f1; }

.page-content { margin: 0 auto; box-sizing: border-box; font-size: 0; position: relative; padding-left: 17px; }
.items { box-sizing: border-box; display: inline-block; padding-right: 17px; vertical-align: top; padding-bottom: 17px; position: relative; }
.item-art { width: 56.0%; }
.item-design { width: 43.8%;  }
.item-technology { width: 52.7%; }
.item-architecture { width: 47.3%; }

.items a,
.items img { display: block; width: 100%; }
.items a { min-height: 356px; position: relative; display: block; background-repeat: no-repeat; background-size: cover; background-position: center; }
.items img { width: 100%; opacity: 0; }
.items .title { position: absolute; right: 8%; bottom: 9%; font: 58px/68px "Myriad Pro", Helvetica; color: #fff; text-shadow: 0 0 2px #666; letter-spacing: 1px; margin-bottom: 5px; margin-right: 5px; }
.items .shares { position: absolute; right: 5%; bottom: 6%; font: 18px/20px "Myriad Pro", Helvetica; color: #fff; word-spacing: 6px; letter-spacing: 1px; margin-bottom: 5px; margin-right: 5px; }


body .footer .footer-top { padding-top: 150px; }
body .footer::after { top: 193px; }


.item-list li { display: block; width: auto; margin-left: 7px; margin-right: 7px; }


@media (max-width: 768px) {
    .items { display: block; width: auto; }
}

@media (max-width: 960px) {
    .items .title  { font-size: 36px }
    .items .shares { font-size: 14px }
}
.footer {
    position: relative; background: #b03c15; color: #aaa;
}
.footer .footer-top    { position: relative; text-align: center; padding: 180px 0; background: #252525; }
.footer .footer-top::after { content: ''; display: block; height: 1px; width: 100%; position: absolute; left: 0; bottom: -1px; background: #737373; border-bottom: 1px solid #252525; }
.footer .footer-top .footer-sns { position: relative; }
.footer .footer-top .footer-sns .sns-group { display: inline-block; position: relative; width: 50%; }
.footer .footer-top .footer-sns .sns-link { padding: 0; width: 49%; display: inline-block; text-align: center; text-decoration: none; }
.footer .footer-top .footer-sns .sns-link i { display: inline-block; width: 150px; height: 150px; vertical-align: top; }
.footer .footer-top .footer-sns .sns-link i.wechat-ico   { background: url(<%= asset_path('split.png') %>) -320px -330px no-repeat; }
.footer .footer-top .footer-sns .sns-link i.weibo-ico    { background: url(<%= asset_path('split.png') %>) -470px -480px no-repeat; }
.footer .footer-top .footer-sns .sns-link i.ios-ico      { background: url(<%= asset_path('split.png') %>) -630px -640px no-repeat; }
.footer .footer-top .footer-sns .sns-link i.android-ico  { background: url(<%= asset_path('split.png') %>) -790px -800px no-repeat; }
.footer .footer-top .footer-sns.show .sns-link { opacity: 1; -webkit-animation: 'zoomin' .6s ease; }

.footer .footer-top .app-download { display:none; text-align: center; padding-top: 80px; }
.footer .footer-top .app-download a { display: inline-block; width: 196px; height: 34px; margin-bottom: 40px; }
.footer .footer-top .app-download a.apple   { background: url(<%= asset_path('split.png') %>) -625px -615px no-repeat; background-size: 1250px auto; }
.footer .footer-top .app-download a.android { background: url(<%= asset_path('split.png') %>) -625px -615px no-repeat; background-size: 1250px auto; }


@-webkit-keyframes 'zoomin' {
    0%   {-webkit-transform: scale(0);}
    50%  {-webkit-transform: scale(0);}
    85%  {-webkit-transform: scale(1.2);}
    100% {-webkit-transform: scale(1);}
}

.footer .footer-top .sns-link { opacity: 0; -webkit-transition: all 0.5s; -moz-transition: all 0.5s; -ms-transition: all 0.5s; transition: all 0.5s;}
.footer .footer-bottom {
    height: 130px; padding-top: 23px; padding-left: 8px; background: #252525; margin-left: 26%;
    font: 10px/20px Helverica; color: #878a8a;
}

@media (max-width: 768px) {
    .footer .footer-bottom { margin-left: 8%; }
    .footer .footer-top    { padding: 80px 0; }
    .footer .footer-top .footer-sns .sns-link i { zoom: 0.7; }
}
@media (max-width: 520px) {
    .footer .footer-top .footer-sns .sns-group { display: block; width: 80%; margin: 0 auto; }
    .footer .footer-top .footer-sns .sns-group:nth-of-type(1) { margin-bottom: 40px; }
    .footer .footer-top    { padding: 40px 0; }
    .footer .footer-top .footer-sns .sns-link { }
    .footer .footer-top .footer-sns .sns-link i { zoom: 0.5; }
    .footer .footer-top .app-download { display:block; }
}
.wechat-ico { display: inline-block; width: 22px; height: 19px; background: url(<%= asset_path('split.png') %>) -290px -300px no-repeat; }
.weibo-ico  { display: inline-block; width: 22px; height: 19px; background: url(<%= asset_path('split.png') %>) -260px -270px no-repeat; }

.footer {
    position: relative; background: #b03c15; color: #aaa;
}
.footer::after { content: ''; display: block; height: 1px; width: 100%; position: absolute; top: 59px; left: 0; background: rgba(230,230,230,0.2); }
.footer .footer-top    { height: 43px; padding-top: 16px; background: #252525; margin-left: 26%; }
.footer .footer-top .sns-link { display: inline-block; vertical-align: top; text-decoration: none; padding: 3px; margin-left: 12px; }
.footer .footer-bottom {
