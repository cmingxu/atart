<li>
<a href="<%= product_path(product) %>" class="img">
  <%= image_tag product.images.first.try(:url), width: '363', height: '206' %>
</a>
<%= link_to product.name, product_path(product), class: 'description' %>
<span class="location">
  <%= product.address %>
</span>
<span class="price"><%= product.price %>CNY</span>
<span class="cart"></span>
</li>
